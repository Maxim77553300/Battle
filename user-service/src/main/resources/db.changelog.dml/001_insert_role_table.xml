    <changeSet id="001.01_add_role_table" author="Maksim Shumkov">
        <insert tableName="role">
            <column name="id" type="varchar(36)" valueComputed="${u_id}"/>
            <column name="name" value="USER"/>
        </insert>
        <insert tableName="role">
            <column name="id" type="varchar(36)" valueComputed="${u_id}"/>
            <column name="name" value="ADMIN"/>
        </insert>
    </changeSet>
</databaseChangeLog>


